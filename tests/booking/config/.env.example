# Freetime Endpoint Test Configuration
# Copy this file to .env and adjust values for your environment

# Base URL of the API server
FREETIME_TEST_BASE_URL=https://pe-api.test

# Resource ID to test (must be a simple_booking enabled resource)
FREETIME_TEST_RESOURCE_ID=106

# Building ID
FREETIME_TEST_BUILDING_ID=10

# Timezone (used for date/time calculations)
FREETIME_TEST_TIMEZONE=Europe/Oslo
